const { getDefaultConfig } = require("metro-config");

module.exports = (async () => {
    const {
        resolver: { sourceExts, assetExts },
    } = await getDefaultConfig.getDefaultValues("metro");
    return {
        resolver: {
            assetExts: assetExts.filter(ext => ext !== "db"),
            sourceExts: [...sourceExts, "db"],
        },
    };
})();