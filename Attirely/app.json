{
  "expo": {
    "name": "Attirely",
    "slug": "Attirely",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {},
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.audreynge.Attirely",
      "infoPlist": {
        "NSCameraUsageDescription": "Attirely needs access to your Camera."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.audreynge.Attirely"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Attirely needs access to your Camera."
        }
      ],
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "44356c18-2526-42dd-9041-64ba1ff90fd8"
      }
    }
  }
}
